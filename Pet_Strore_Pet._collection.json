      "name": "create Pet",
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "\r",
              "let dynamicPetId = Math.floor(Math.random() * 1000000) + 1;\r",
              "let dynamicTagId = Math.floor(Math.random() * 1000) + 1;\r",
              "\r",
              "pm.collectionVariables.set(\"petId\", dynamicPetId);\r",
              "pm.collectionVariables.set(\"tagId\", dynamicTagId);\r",
              "\r",
              "\r",
              "\r",
              "\r",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        },
        {
          "listen": "test",
          "script": {
            "exec": [
              "let res = pm.response.json();\r",
              "\r",
              "pm.expect(res.id).to.eql(Number(pm.collectionVariables.get(\"petId\")));\r",
              "\r",
              "pm.collectionVariables.set(\"petId\", res.id);\r",
              "\r",
              "\r",
              "// 1. Status code validation\r",
              "pm.test(\"Status code is 200\", function () {\r",
              "    pm.response.to.have.status(200);\r",
              "});\r",
              "\r",
              "// 2. Response should be JSON\r",
              "pm.test(\"Response is in JSON format\", function () {\r",
              "    pm.response.to.be.json;\r",
              "});\r",
              "\r",
              "let jsonData = pm.response.json();\r",
              "\r",
              "// 3. ID should exist and be a number\r",
              "pm.test(\"Pet ID exists and is a number\", function () {\r",
              "    pm.expect(jsonData.id).to.exist;\r",
              "    pm.expect(jsonData.id).to.be.a(\"number\");\r",
              "});\r",
              "\r",
              "// 4. Name should match request body\r",
              "pm.test(\"Pet name is correct\", function () {\r",
              "    let requestBody = pm.request.body.raw;\r",
              "    let reqJson = JSON.parse(requestBody);\r",
              "\r",
              "    pm.expect(jsonData.name).to.eql(reqJson.name);\r",
              "});\r",
              "\r",
              "// 5. Category name should be correct\r",
              "pm.test(\"Category name is correct\", function () {\r",
              "    pm.expect(jsonData.category.name).to.eql(\"Dogs\");\r",
              "});\r",
              "\r",
              "// 6. Photo URL should not be empty\r",
              "pm.test(\"Photo URL is present\", function () {\r",
              "    pm.expect(jsonData.photoUrls.length).to.be.greaterThan(0);\r",
              "});\r",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
