    {
      "name": "create_User",
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "let random = Math.random().toString(36).substring(4);\r",
              "\r",
              "let name = \"mohit\"+random;\r",
              "let email =\"mohit\"+random+\"@gmail.com\";\r",
              "\r",
              "pm.environment.set(\"name\", name);\r",
              "pm.environment.set(\"email\", email);"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        },
        {
          "listen": "test",
          "script": {
            "exec": [
              "\r",
              "let data = pm.response.json();\r",
              "\r",
              "pm.environment.set(\"userId\", data.id);"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "raw",
          "raw": "{\r\n  \"name\": \"{{name}}\",\r\n  \"email\": \"{{email}}\",\r\n  \"gender\": \"male\",\r\n  \"status\": \"inactive\"\r\n}\r\n",
          "options": {
            "raw": {
              "language": "json"
            }
          }
        },
        "url": {
          "raw": "{{gorest_Url}}/public/v2/users",
          "host": [
            "{{gorest_Url}}"
          ],
          "path": [
            "public",
            "v2",
            "users"
          ]
        }
